{
  "compilerOptions": {
      "target": "es2015",
      "module": "commonjs",
      "moduleResolution": "node",
      "lib": [
          "es2019",
          "dom"
      ],
      "jsx": "react",
      "noEmit": true,
      "pretty": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "noErrorTruncation": true,
      "skipLibCheck": true,
      "strict": true,
      "noUnusedLocals": false,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "baseUrl": ".",
      "paths": {
          "~/*": [
              "./*"
          ]
      },
  },
}