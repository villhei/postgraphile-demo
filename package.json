{
  "name": "postgraphile-demo",
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "start": "npm-run-all --parallel \"start:server\" \"start:client\" \"start:codegen\"",
    "start:server": "ts-node server.ts",
    "start:client": "parcel src/index.html",
    "start:codegen": "graphql-codegen --watch --config codegen.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript": "^4.1.3",
    "@graphql-codegen/typescript-operations": "^4.4.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@parcel/watcher": "^2.5.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^22.12.0",
    "eslint": "^8.12.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.13.3",
    "prettier": "^3.4.2",
    "process": "^0.11.10",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@apollo/client": "^3.12.8",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@parcel/core": "^2.13.3",
    "@types/cors": "^2.8.17",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "postgraphile": "^4.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.3"
  }
}
