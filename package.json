{
  "name": "postgraphile-demo",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "start": "npm-run-all --parallel \"start:server\" \"start:client\"",
    "start:server": "ts-node server.ts",
    "start:client": "parcel src/index.html",
    "start:codegen": "graphql-codegen --watch --config codegen.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/near-operation-file-preset": "^2.2.9",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-codegen/typescript-react-apollo": "^3.2.11",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "eslint": "^8.12.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.4.0",
    "prettier": "^2.6.1",
    "process": "^0.11.10",
    "ts-node": "^10.7.0"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@types/cors": "^2.8.12",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "postgraphile": "^4.12.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.6.3"
  }
}
